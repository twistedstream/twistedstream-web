{{#> main_layout}}
  {{#*inline "head_scripts"}}
  {{/inline}}

  {{#*inline "body"}}
    <div class="container-fluid">
      <div class="row mt-2">
        <div class="col text-center">
          <img src="/images/logo-lg.png" class="img-fluid">
        </div>
      </div>

      <div class="row mt-3">
        <div class="col text-center">
          <p class="h3">
          {{#each links}}
            <a href="{{local_url}}" target="_blank">{{name}}</a>
            {{#unless @last}}|{{/unless}}  
          {{/each}}
          </p>
        </div>
      </div>

      <div class="row mt-3">
        <div class="col text-center small">
          <p>
            powered by:<br>
            {{#each poweredBys }}
            <a href="{{ url }}" target="_blank">{{ name }}</a>,
            {{/each}}
            and bananas
          </p>
        </div>
      </div>
    </div>
  {{/inline}}

  {{#*inline "body_scripts"}}
  {{/inline}}
{{/main_layout}}
